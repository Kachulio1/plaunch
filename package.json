{
  "name": "plaunch",
  "description": "A simple CLI for improving your productivity ",
  "version": "1.0.0",
  "author": "kachulio1 @Kachulio1",
  "bin": {
    "plaunch": "./bin/run"
  },
  "bugs": "https://github.com/Kachulio1/plaunch/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "execa": "^2.0.3",
    "find-up": "^4.1.0",
    "listr": "^0.14.3",
    "resolve-dir": "^1.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/Kachulio1/plaunch",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "plaunch",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/Kachulio1/plaunch",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
